    "coveralls": "^3.0.9",
    "jest": "^26.1.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "tslib": "^2.0.0",
    "tslint": "^6.0.0",
    "tslint-config-alloy": "^0.2.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
