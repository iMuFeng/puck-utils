{
  "name": "@puckjs/utils",
  "description": "A basic development library",
  "version": "2.2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "http://github.com/iMuFeng/puck-utils",
  "author": "mufeng.me@gmail.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:iMuFeng/puck-utils.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "test": "npm run lint && jest",
