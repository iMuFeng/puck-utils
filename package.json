{
  "name": "@puckjs/utils",
  "description": "Personal assistant tool library",
  "version": "1.5.2",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "http://github.com/iMuFeng/puck-utils",
