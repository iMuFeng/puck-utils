    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "coveralls": "^3.0.7",
    "jest": "^24.0.19",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "tslint-config-alloy": "^0.2.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)test)\\.ts$",
